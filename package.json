{
  "name": "movecord",
  "version": "0.0.1",
  "private": true,
  "description": "Discord bot to move messages between channels, or between servers",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only src/index.ts",
    "serve": "node build/index.js",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/index.ts"
  },
  "author": "",
  "license": "0BSD",
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@types/node": "^17.0.23",
    "discord-api-types": "^0.31.0",
    "discord.js": "^13.6.0",
    "discordx": "^9.5.2",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
